# Deny all other file extensions to prevent any compromised files to be executed

<FilesMatch "(?<!jpg|jpeg|png|gif|webp|mp3|mp4|flv|webm|swf)$">
    <IfModule mod_authz_core.c>
        # Apache >= 2.4
        Require all denied
    </IfModule>

    <IfModule !mod_authz_core.c>
        # Apache <= 2.2
        Deny from all
    </IfModule>
</FilesMatch>
